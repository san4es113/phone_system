<p id="notice"><%= notice %></p>

<h1>Passlosts</h1>

<table class="table">
  <thead>
    <tr>
      <th>T-R Separation</th>
      <th>Carrier Frequency</th>
      <th>TX Antenna Height</th>
      <th>RX Antenna Height</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @passlosts.each do |passlost| %>
      <tr>
        <td><%= passlost.trSeparation %> Km</td>
        <td><%= passlost.carrierFrequency %> MHz</td>
        <td><%= passlost.txAntennaHeight %> Meters</td>
        <td><%= passlost.rxAntennaHeight %> Meters</td>
        <td><%= link_to 'Show', passlost , :class => "btn btn-info"%></td>
        <td><%= link_to 'Edit', edit_passlost_path(passlost) ,:class => "btn btn-warning"%></td>
        <td><%= link_to 'Destroy', passlost, method: :delete, data: { confirm: 'Are you sure?' } ,:class => "btn btn-danger"%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Create new parameters', new_passlost_path %>
